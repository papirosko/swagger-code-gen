{
  "appenders": {
    "out": {
      "type": "stdout",
      "layout": {
        "type": "pattern",
        "pattern": "%[%d [%p] (%f{1}:%l:%o) %c%] %m"
      }
    }
  },
  "categories": {
    "default": {
      "appenders": [
        "out"
      ],
      "level": "DEBUG",
      "enableCallStack": true
    }
  }
}
